// Chech no. equal or not.
#include<stdio.h> 
int main() 
{ 
  int a,A; 
  printf("enter numers:\n"); 
  scanf("%d %d",&a,&A); 
  if(a==A) 
  printf("equal"); 
  else 
  printf("not equal"); 
  return 0; 
} 
  
// Positive ,negative & zero 
#include<stdio.h> 
int main() 
{ 
  int a; 
  printf("enter number:\n"); 
  scanf("%d",&a); 
  if(a<0) 
  printf("%d is positive",a); 
  else if (a>0) 
  printf("%d is negative",a); 
  else 
  printf("%d is zero",a); 
  return 0; 
} 
  
// To check chararacter is vowel or not.
#include<stdio.h> 
int main() 
{ 
  int ch; 
  printf("enter character:\n"); 
  scanf("%c",&ch); 
  if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U') 
  printf("%c is vowel",ch); 
  else 
  printf("%c is not vowel",ch); 
  return 0; 
} 
  
// Leap year or not 
#include<stdio.h> 
int main() 
{ 
  int leap_yr,next_leap; 
  printf("enter year:\n"); 
  scanf("%d",&leap_yr); 
  if((leap_yr%4==0))
  printf("%d is leap year\n",leap_yr); 
  else 
  printf("%d is not leap year\n",leap_yr); 
  next_leap=leap_yr+4; 
  printf("next leap year is %d",next_leap); 
  return 0; 
} 
 
// Largest of 3 numbers 
#include<stdio.h> 
int main() 
{ 
  int a,b,c; 
  printf("enter 3 no.:\n"); 
  scanf("%d %d %d",&a,&b,&c); 
  if(a>b && a>c) 
  printf("Greater is %d",a); 
  else if (b>a && b>c) 
  printf("Greater is %d",b); 
  else 
  printf("Greater is %d",c); 
  return 0; 
} 
  
// Program to find Coordinates. 

#include<stdio.h> 
int main() 
{ 
  int x,y; 
  printf("enter x&y coord:\n"); 
  scanf("%d %d",&x,&y); 
  if(x>0) 
  { 
    if(y>0) 
    printf("1st"); 
    else 
    printf("4th"); 
  } 
  else if(x<0) 
  { 
    if(y>0) 
    printf("2nd"); 
    else 
    printf("3rd"); 
  } 
  else if(x==0 && y==0) 
  printf("origin"); 
  else 
  printf("lies on axis"); 
  return 0; 
} 
  
// Program to measure temperature intensity. 
#include<stdio.h> 
int main() 
{ 
  float temp; 
  printf("enter temp:\n"); 
  scanf("%f",&temp); 
  if(temp<0) 
  printf("Freezing"); 
  else if(temp>0 && temp<10) 
  printf("very cold"); 
  else if(temp>10 && temp<20) 
  printf("cold"); 
  else if(temp>20 && temp<30) 
  printf("Normal"); 
  else 
  printf("hot"); 
  return 0; 
} 
  
// If triangle is equilateral,isosceles or scalene. 
#include<stdio.h> 
int main() 
{ 
  float a,b,c; 
  printf("enter sides:\n"); 
  scanf("%d %d %d",&a,&b,&c); 
  if(a==b==c) 
  printf("equilateral"); 
  else if(a==b || a==c || b==c) 
  printf("isosceles"); 
  else 
  printf("scalene"); 
  return 0; 
} 
 
