#include<stdio.h>
int main()
{
  char c[20];
  int ID, unit;
  float amt, cpu1, cpu2, cpu3, cpu4;
  printf("name  id  unit");
  scanf("%s %d %d", &c, &ID, &unit);
  if(unit<=199)
  {
  cpu1 = 1.2;
  amt = cpu1* unit;
  }
  else if(unit>=200 && unit<400)
  {
  cpu2 = 1.5;
  amt = (1.2*199) + (cpu2*(unit-199));
  }
  else if(unit>=400 && unit<600)
  {
  cpu3=1.8;
  amt = (1.2*199) + (1.5*199) + (cpu3*(unit-399));
  }
  else
  {
  cpu4=2.0;
  
  amt = (1.2*199) + (1.5*199) + (1.8*199) + (cpu4*(unit-599));
  }
  
  if(amt>=400)
  amt = amt + amt*0.15;
  if(amt<=100)
  amt = 100;
  
  printf("%f", amt);
  return 0;
}

// Program to print day of the week.
#include<stdio.h>
int main ()
{
  enum month {SUN=1,MON,TUE,WED,THUR,FRI,SAT};
  enum month day;
  printf("enter day:\n");
  scanf("%d",&day);
  switch (day)
  {
      case 1: printf("SUN");
      break;
      case 2: printf("MON");
      break;
      case 3: printf("TUE");
      break;
      case 4: printf("WED");
      break;
      case 5: printf("THUR");
      break;
      case 6: printf("FRI");
      break;
      case 7: printf("SAT");
      break;
  }
  return 0;
}

// Condense specified number of total days into years,weeks & days.
#include<stdio.h>
int main ()
{
  int a;
  int yrs,modyrs, weeks, modweeks, days;
  printf("enter total no. of days:");
  scanf("%d", &a);
  modyrs=a%365;
  yrs=a/365;
  modweeks=modyrs%7;
  weeks=modyrs/7;
  days=modweeks;
  printf("%d %d %d", yrs, weeks, days);
  return 0;
}

// Program to check available mobiles in the range of the of user budget.
#include<stdio.h>
#include<math.h>
int main()
{
  int a,b;
  int Vivo=20000, Oppo=30000, Redmi=100000, Nokia=30000, Oneplus=50000, Samsung=40000;
  printf("enter a and b");
  scanf("%d %d", &a, &b);
  if(Vivo>=a && Vivo<=b)
  printf("Vivo is available\n");
  if(Oppo>=a && Oppo<=b)
  printf("Oppo is available\n");
  if(Redmi>=a && Redmi<=b)
  printf("Redmi is available\n");
  if(Nokia>=a && Nokia<=b)
  printf("Nokia is available\n");
  if(Oneplus>=a && Oneplus<=b)
  printf("Oneplus is available\n");
  if(Samsung>=a && Samsung<=b)
  printf("Samsung is available");
  return 0;
}

// Calculator program.
#include<stdio.h>
#include<math.h>
int main()
{
  int a,b,c;
  char ch;
  
  printf("enter two numbers and operation\n");
  scanf("%d %c %d",&a,&ch,&b);
  switch(ch)
  {
    case '+':
    c=a+b;
    break;
    case '-':
    c=a-b;
    break;
    case '*':
    c=a*b;
    break;
    case '/':
    c=a/b;
    break;
  }
  printf("%d",c);
  return 0;
}





